# Auto-load environment variables for this worktree
# Uses .env file from main repository

# Source the .env file if it exists
if [ -f .env ]; then
  export $(cat .env | grep -v '^#' | xargs)
fi

# Alternatively, source from main repo
if [ -f /Users/ricore/strikehouse/.env ]; then
  export $(cat /Users/ricore/strikehouse/.env | grep -v '^#' | xargs)
fi
